{
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "scripting",
    "activeTab",
    "webNavigation"
  ],
  "background": {
    "service_worker": "navigationListener.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*/games/zip*"],
      "js": ["zip-content.js"]
    }
  ],
  "action": {
    "default_popup": "hello.html"
  }
}